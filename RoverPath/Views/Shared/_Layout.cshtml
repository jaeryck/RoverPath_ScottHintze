<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title - My ASP.NET Application</title>
	@Styles.Render("~/Content/css")
	@Scripts.Render("~/bundles/modernizr")
</head>
<body>
	<nav class="navbar fixed-top navbar-dark bg-dark">
		<div class="container-fluid">
			<a href='@Url.Action("Index")' class="navbar-brand">NASA RoverPath Test</a>
			<ul class="nav">
				<li class="nav-item">
					<a href='@Url.Action("Requirements")' class="nav-link">Requirements</a>
				</li>
				<li class="nav-item">
					<a href='@Url.Action("ProgramLogic")' class="nav-link">Program Logic</a>
				</li>
			</ul>
			
			
		</div>
		

	</nav>
	<div class="container body-content">
		@RenderBody()
		<hr />
		<footer>
			<p>&copy; @DateTime.Now.Year - NASA</p>
		</footer>
	</div>

	@Scripts.Render("~/bundles/jquery")
	@Scripts.Render("~/bundles/bootstrap")
	@Scripts.Render("~/bundles/jqueryval")
	@RenderSection("scripts", required: false)
</body>
</html>
